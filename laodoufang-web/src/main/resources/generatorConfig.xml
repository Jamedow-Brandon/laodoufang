        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://106.14.210.31:3306/laodoufang?useUnicode=true&amp;characterEncoding=utf-8"
                        userId="root"
                        password="Zhangliang@520"/>

        <!-- 生成实体类地址 -->
        <javaModelGenerator targetPackage="com.jamedow.laodoufang.entity"
                            targetProject="/Users/brandon/">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="false"/>
        </javaModelGenerator>

